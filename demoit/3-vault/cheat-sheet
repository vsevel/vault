
kubectl delete service vault
kubectl apply -f vault-service.yaml
kubectl apply -f vault-ingress.yaml

add '127.0.0.1       myvault.mycompany.io' to /etc/hosts

url=http://myvault.mycompany.io/v1/sys/health
curl -s $url | jq

token=XXX
url=http://myvault.mycompany.io/v1/secret/foo
curl -s --header "X-Vault-Token: $token" $url | jq  '.data' 



